{
  "name": "@wildegor/e-shop-nodepack",
  "version": "1.0.0",
  "description": "eShopNodepack - common packages for Node.js projects",
  "author": "kartashov_egor96@mail.ru",
  "private": false,
  "license": "MIT",
  "main": "modules/index.js",
  "types": "modules/index.d.ts",
  "repository": "git://github.com/wildegor/e-shop-nodepack.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/wildegor",
    "repository": "https://github.com/wildegor/e-shop-nodepack.git"
  },
  "files": [
    "modules"
  ],
  "resolutions": {
    "string-width": "4.2.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "gen:proto": "grpc_tools_node_protoc --ts_proto_out=./src/@types/proto --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false,esModuleInterop=true --proto_path=./proto ./proto/*.proto",
    "commit": "git-cz",
    "prebuild": "rimraf modules",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch --path tsconfig.json",
    "start:debug": "nest start --debug --watch",
    "dev:lint": "eslint --quiet src/**/*.ts",
    "dev:token": "node scripts/npm-token.js",
    "ci:token": "node scripts/ci-token.js",
    "dev:fix": "yarn dev:lint -- --fix",
    "dev:check": "tsc --noEmit --project tsconfig.build.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "release": "semantic-release",
    "prepare": "husky install && yarn build"
  },
  "devDependencies": {
    "@fastify/static": "7.0.0",
    "@grpc/grpc-js": "1.10.1",
    "@grpc/proto-loader": "0.7.10",
    "@jest/globals": "29.6.2",
    "@nestjs/cli": "10.2.1",
    "@nestjs/common": "10.2.10",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.10",
    "@nestjs/platform-fastify": "10.3.1",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/terminus": "10.2.1",
    "@prisma/client": "5.10.2",
    "@semantic-release/git": "10.0.1",
    "@types/luxon": "3.4.2",
    "@types/microseconds": "0.2.2",
    "@types/node": "20.10.6",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "axios": "1.6.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "grpc-tools": "1.12.4",
    "grpc_tools_node_protoc_ts": "5.3.3",
    "husky": "8.0.3",
    "ioredis": "5.3.2",
    "jest": "29.6.2",
    "luxon": "3.4.4",
    "microseconds": "0.2.0",
    "minio": "7.1.3",
    "mongodb": "6.5.0",
    "nestjs-cls": "3.6.0",
    "nice-grpc": "2.1.7",
    "prettier": "2.3.2",
    "prisma": "5.10.2",
    "prom-client": "15.1.1",
    "rabbitmq-client": "4.5.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.8.1",
    "semantic-release": "21.1.2",
    "source-map-support": "0.5.20",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-proto": "1.168.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "5.3.3",
    "uuid": "9.0.1",
    "winston": "^3.13.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
